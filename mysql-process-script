ALTER SESSION SET NLS_DATE_FORMAT='DD-MM-RRRR HH24:MI:SS';

insert into farmertest.TBL_ADMINS
(CREATE_TIME)
select to_char(to_date(CREATE_TIME,'RRRR-MM-DD HH24:MI:SS'),'DD-MM-RRRR HH24:MI:SS') from TBL_ADMINS;

select to_char(to_date(CREATE_TIME,'RRRR-MM-DD HH24:MI:SS'),'DD-MM-RRRR HH24:MI:SS') from TBL_ADMINS;

======================

select id, count(id) from tbl_farmer having count(id)>1 group by id order by id desc;
----
delete from tbl_farmer where id in (select id from tbl_farmer 
            group by id having count(*) > 1)
and rowid not in (select min(rowid) from tbl_farmer
group by id having count(*) > 1);

-------------

CREATE TABLE books (
  id      NUMBER        GENERATED BY DEFAULT ON NULL AS IDENTITY,
  title   VARCHAR2(100) NOT NULL
);

--------------

UPDATE tbl_farmer
set name_en=REPLACE(name_en, '''', '');
